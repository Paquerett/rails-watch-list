<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-10 col-sm-offset-1">
      <h1>Create a new bookmark</h1>
        <%= simple_form_for @bookmark do |f| %>
          <%= f.input :comment %>
          <%= f.association :lists %>
          <%= f.association :movies %>
          <%= f.button :submit %>
        <% end %>
    </div>
  </div>
</div>
